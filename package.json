{
  "name": "esgisoftware",
  "version": "1.0.0",
  "description": "Early Childhood Education",
  "repository": {
    "name": "esgi-ui-automation-next",
    "type": "git",
    "url": ""
  },
  "main": "./node_modules/.bin/wdio",
  "scripts": {
    "allure:history": "mkdir -p allure-report/history ./test/reports/allure-results/history && cp -a allure-report/history ./test/reports/allure-results/",
    "allure-report": "node_modules/.bin/allure generate --clean ./test/reports/allure-results && allure open",
    "junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html",
    "test-sauce": "wdio ./test/config/wdio.sauce.conf.js",
    "test-browserstack": "wdio ./test/config/wdio.browserstack.conf.js",
    "clean": "gulp clean",
    "slacksummaryreport": "gulp slacksummaryreport",
    "test-local": "wdio ./test/config/wdio.local.conf.js",
    "login": "wdio ./test/config/wdio.local.conf.js --suite login",
    "flashcards": "wdio ./test/config/wdio.local.conf.js --suite flashcards",
    "gradescale": "wdio ./test/config/wdio.local.conf.js --suite gradescale",
    "test": "wdio ./test/config/wdio.local.conf.js",
    "classtotals": "wdio ./test/config/wdio.reports.conf.js --suite classtotals",
    "parentletter": "wdio ./test/config/wdio.reports.conf.js --suite parentletter",
    "itemanalysis": "wdio ./test/config/wdio.reports.conf.js --suite itemanalysis",
    "studentdetail": "wdio ./test/config/wdio.reports.conf.js --suite studentdetail",
    "studentprogress": "wdio ./test/config/wdio.reports.conf.js --suite studentprogress",
    "reports1": "wdio ./test/config/wdio.reports.conf.js --suite reports1",
    "reports2": "wdio ./test/config/wdio.reports.conf.js --suite reports2",
    "reports3": "wdio ./test/config/wdio.reports.conf.js --suite reports3",
    "reports": "concurrently \"npm:reports1\" \"npm:reports2\" \"npm:reports3\"",
    "criticalpath": "wdio ./test/config/wdio.local.conf.js --suite criticalpath",
    "prodsmoke": "wdio ./test/config/wdio.prod.conf.js --suite prodsmoke",
    "test-mobile": "wdio ./test/config/wdio.appium.conf.js",
    "startchrome": "docker-compose up -d",
    "stopchrome": "docker stop chrome || true && docker rm chrome || true"
  },
  "keywords": [
    "selenium",
    "web testing",
    "webdriver",
    "webdriverio",
    "automation",
    "e2e",
    "test",
    "mocha"
  ],
  "dependencies": {
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@wdio/allure-reporter": "^7.2.0",
    "@wdio/appium-service": "^7.2.0",
    "@wdio/browserstack-service": "^7.2.0",
    "@wdio/cli": "^7.2.0",
    "@wdio/junit-reporter": "^7.2.0",
    "@wdio/local-runner": "^7.2.0",
    "@wdio/mocha-framework": "^7.2.0",
    "@wdio/sauce-service": "^6.0.4",
    "@wdio/selenium-standalone-service": "^7.2.0",
    "@wdio/spec-reporter": "^7.2.0",
    "@wdio/sync": "^7.2.0",
    "allure-commandline": "^2.13.0",
    "axios": "^0.21.1",
    "babel-plugin-source-map-support": "^2.1.3",
    "compare-pdf": "^1.1.4",
    "concurrently": "^6.0.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "grunt": "^1.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-mocha": "^1.2.0",
    "grunt-webdriver": "^3.0.0",
    "gulp": "^4.0.2",
    "junit-viewer": "^4.11.1",
    "node-excel-to-json": "0.0.3",
    "selenium-standalone": "^6.20.0",
    "shelljs": "^0.8.4",
    "underscore": "^1.11.0",
    "wdio-chromedriver-service": "^6.0.2",
    "wdio-image-comparison-service": "^2.0.0",
    "wdio-json-reporter": "^2.0.0"
  }
}
